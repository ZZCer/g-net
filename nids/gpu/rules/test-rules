#-----------------
# COMMUNITY RULES
#-----------------
alert tcp $EXTERNAL_NET any -> $HOME_NET any (msg:"BLACKLIST known malicious FTP login banner - 0wns j0"; content:"h"; content:"e"; content:"r"; content:"s";)
alert tcp $EXTERNAL_NET any -> $HOME_NET any (msg:"BLACKLIST known malicious FTP login banner - she"; content:"s"; content:"h"; content:"e";)
alert tcp $EXTERNAL_NET any -> $HOME_NET any (msg:"BLACKLIST known malicious FTP login banner - he"; content:"h"; content:"e";)
alert tcp $EXTERNAL_NET any -> $HOME_NET any (msg:"BLACKLIST known malicious FTP login banner - she"; content:"h"; content:"i"; content:"s";)
alert tcp $EXTERNAL_NET any -> $HOME_NET any (msg:"BLACKLIST known malicious FTP login banner - her"; content:"hers";)
alert tcp $EXTERNAL_NET any -> $HOME_NET any (msg:"BLACKLIST known malicious FTP login banner - he"; content:"he";)
alert tcp $EXTERNAL_NET any -> $HOME_NET any (msg:"BLACKLIST known malicious FTP login banner - she"; content:"she";)
alert tcp $EXTERNAL_NET any -> $HOME_NET any (msg:"BLACKLIST known malicious FTP login banner - er"; content:"er";)
